VUE_APP_NEED_CHANGEBAR = false
# 是否隐藏用户信息
VUE_APP_HIDE_USERINFO = false
# 是否开启全局长链接
VUE_APP_NEED_WEBSOCKET = false
# 是否开启声音通知
VUE_APP_NEED_SOUND_ALARM = false
# 默认存储前缀
VUE_APP_DEFAULT_KEY_PREFIX = ''

# 是否默认浏览器图标
VUE_APP_IS_WEB_FAVICON = false