NODE_ENV = 'production'
NODE_ENV_NAME = '生产环境'
VUE_APP_BASE_API = '/'
# 产品名称
VUE_APP_PRODUCT = 'isop-sop'
# test开启全部菜单
VUE_APP_MENUDIY = ''
# 默认主题颜色 ( light 与 dark )
VUE_APP_CUSTOM_STYLE = ''
# 主题入口(目前：标品为 standard 移动相关为 china-mobile)
VUE_APP_THEME_ENTRY = 'standard'
# 默认进入的页面
VUE_APP_DEFAULT_PAGE = 'login'
